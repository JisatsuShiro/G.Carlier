                    <!-- Including the TagCloud.js script CDN in the document    -->
                    <script src="https://cdn.jsdelivr.net/npm/TagCloud@2.2.0/dist/TagCloud.min.js"> </script>                    <script>
                        const Texts = [
                            'HTML', 'CSS', 'JAVASCRIPT',
                            'SYMFONY', 'JAVA', 'REACT',
                            'PYTHON', 'AGILE', 'SQL',
                            'THREE.JS', 'ANGULAR', 'PHP',
                            'TAILWIND', 'BOOTSTRAP', 'API', 'GIT'
                        ];
                        var tagCloud = TagCloud('.Sphere', Texts, {
                            // Sphere radius in px
                            radius: 170,
                            // animation speed
                            // slow, normal, fast
                            maxSpeed: 'slow',
                            initSpeed: 'slow',
                            // Rolling direction [0 (top) , 90 (left), 135 (right-bottom)]
                            direction: 135,
                            // interaction with mouse or not [Default true (decelerate to rolling init speed, and keep rolling with mouse).]
                            keep: true
                        });
                        // Giving color to each text in sphere
                        var color = '#FF5733 ';
                        var font_size = 'bold';
                        document.querySelector('.Sphere').style.color = color;
                        document.querySelector('.Sphere').style.fontWeight = font_size;

                        // select compétence pour l'afficher
                        let rootEl = document.querySelector('.Sphere');
                        let onPause = false;
                        rootEl.addEventListener('click', function clickEventHandler(e) {
                            var clickedTechno = e.target.innerHTML;
                            var progressBar = document.querySelectorAll(".progressBar");

                            for (i=0; i < progressBar.length; i++) {
                                if(progressBar[i].classList.contains(clickedTechno)){
                                    progressBar[i].classList.remove("hidden")
                                } else {
                                    progressBar[i].classList.add("hidden");
                                }
                            }
                            //met en pause/resume la rotation
                            if (e.target.className === 'tagcloud--item') {
                                if (onPause === true) {
                                    tagCloud.resume();
                                    onPause = false;
                                } else if (onPause === false){
                                    tagCloud.pause();
                                    onPause = true;
                                }
                            }
                        });
                    </script>
